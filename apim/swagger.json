{
  "swagger": "2.0",
  "info": {
    "title": "Echo API",
    "description": "A simple echo API that returns a timestamp",
    "version": "1.0.0"
  },
  "host": "echo-api.example.com",
  "basePath": "/",
  "schemes": [
    "https"
  ],
  "paths": {
    "/echo": {
      "get": {
        "summary": "Get current timestamp",
        "description": "Returns the current timestamp",
        "operationId": "getTimestamp",
        "produces": [
          "application/json"
        ],
        "responses": {
          "200": {
            "description": "Successful operation",
            "schema": {
              "type": "object",
              "properties": {
                "timestamp": {
                  "type": "string",
                  "format": "date-time"
                }
              }
            }
          }
        }
      }
    }
  }
} 